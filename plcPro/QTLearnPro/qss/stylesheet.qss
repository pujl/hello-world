QGroupBox
{
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #E0E0E0, stop: 1 #FFFFFF);
    border: 3px solid gray;
    border-radius: 6px;
    margin-top: 16px;
    padding:-16 6px 0 6px;
    left:10px;
    font-family:"黑体";
    font-size:35px;
}

QGroupBox::title
{
    subcontrol-origin:margin;
    subcontrol-position:top center;
    left:0px;
    margin-left:0px;
    padding:0 1px;

}

QTableWidget
{
    background: rgb(255,255,255);
    gridline-color:rgb(0,0,0);
    alternate-background-color:rgb(48,51,55);
    selection-color: green;
    selection-background-color:qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgb(200,200,200),stop:1 rgb(255,255,255));
}


QHeaderView::section
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgb(200,200,200),stop:1 rgb(255,255,255));
    color: rgb(0,0,0);
    padding-left: 4px;border: 1px solid rgb(0,0,0);
}

QScrollBar:vertical
{
    border: 0px solid grey;
    background: transparent;
    width: 15px;
    margin: 22px 0 22px 0;
}
QScrollBar::handle:vertical
{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgb(46,46,46),stop:1 rgb(66,66,66));
    min-height: 20px;
}
QScrollBar::add-line:vertical
{
    border: 0px solid grey;
    background: rgb(66,66,66);
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical
{
    border: 0px solid grey;
    background: rgb(56,56,56);
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical
{
    border: 0px solid grey;
    width: 3px;
    height: 3px;
    background: rgb(46,46,46);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    background: none;
}
